// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Sighting {
  id Int @id @default (autoincrement())
  createdAt DateTime @default(now())
  latitude Float
  longitude Float
}

model User {
  id Int @id @default (autoincrement())
  createdAt DateTime @default(now())
  level1 Int
  level2 Int
  level3 Int
  level4 Int
  level5 Int
  level6 Int
}